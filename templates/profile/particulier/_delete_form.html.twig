<form method="post" action="{{ path('admin_user_delete', {'id': particulier.user.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ particulier.user.id) }}">
    <button class="btn">Delete</button>
</form>
